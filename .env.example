# Ultravox Speech-to-Speech Configuration

# API Configuration
ULTRAVOX_API_KEY=your_ultravox_api_key_here

# Call Type Configuration
# Set to 'agent' for agent-specific calls or 'generic' for generic calls
ULTRAVOX_CALL_TYPE=agent

# Agent-specific Configuration (required when CALL_TYPE=agent)
ULTRAVOX_AGENT_ID=your_agent_id_here

# Audio Configuration
ULTRAVOX_SAMPLE_RATE=8000
ULTRAVOX_CLIENT_BUFFER_SIZE_MS=60

# Generic Call Configuration (used when CALL_TYPE=generic)
ULTRAVOX_SYSTEM_PROMPT=You are a helpful AI assistant.
ULTRAVOX_TEMPERATURE=0
ULTRAVOX_MODEL=fixie-ai/ultravox
ULTRAVOX_VOICE=Shaun
ULTRAVOX_RECORDING_ENABLED=false
ULTRAVOX_JOIN_TIMEOUT=30s
ULTRAVOX_MAX_DURATION=3600s

# External Voice Configuration (optional for generic calls)
# Set the provider: elevenlabs, cartesia, lmnt, or generic
ULTRAVOX_EXTERNAL_VOICE_PROVIDER=

# ElevenLabs Configuration
ULTRAVOX_ELEVENLABS_VOICE_ID=
ULTRAVOX_ELEVENLABS_MODEL=eleven_monolingual_v1
ULTRAVOX_ELEVENLABS_SPEED=1.0
ULTRAVOX_ELEVENLABS_USE_SPEAKER_BOOST=true

# Cartesia Configuration
ULTRAVOX_CARTESIA_VOICE_ID=
ULTRAVOX_CARTESIA_MODEL=cartesia-1
ULTRAVOX_CARTESIA_SPEED=1.0

# LMNT Configuration
ULTRAVOX_LMNT_VOICE_ID=
ULTRAVOX_LMNT_MODEL=lmnt-1
ULTRAVOX_LMNT_SPEED=1.0
ULTRAVOX_LMNT_CONVERSATIONAL=true

# Generic Voice Configuration
ULTRAVOX_GENERIC_VOICE_URL=
ULTRAVOX_GENERIC_VOICE_HEADERS={}
ULTRAVOX_GENERIC_VOICE_BODY={}
ULTRAVOX_GENERIC_VOICE_SAMPLE_RATE=24000
ULTRAVOX_GENERIC_VOICE_WPM=150
ULTRAVOX_GENERIC_VOICE_MIME_TYPE=audio/wav
ULTRAVOX_GENERIC_VOICE_AUDIO_FIELD=audio

# Tools Configuration (JSON string for generic calls)
ULTRAVOX_SELECTED_TOOLS=

# VAD Settings (JSON string for generic calls)
ULTRAVOX_VAD_SETTINGS=

# Server Configuration
PORT=6031
